{
  "name": "@ones-ai/performace-auto-test",
  "version": "1.1.0-alpha.8",
  "description": "网站性能自动化测试，一键输出多工具性能报告",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest --detectOpenHandles",
    "coverage": " jest --coverage",
    "clean": "rimraf dist && echo 'clean dist ok'",
    "build": "babel src --out-dir dist",
    "watch": "npm run build -- -w",
    "lint": "eslint src",
    "verfify": "lint-staged",
    "prepublish": "npm run test && npm run clean && npm run build",
    "release": "standard-version --release-as patch",
    "release-minor": "standard-version --release-as minor",
    "release-major": "standard-version --release-as major",
    "release-beta": "standard-version  --prerelease beta",
    "release-alpha": "standard-version  --prerelease alpha"
  },
  "bin": {
    "perf-test": "bin/perf-test"
  },
  "author": "hohozhu",
  "license": "ISC",
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "\\.[jt]sx?$": "babel-jest",
      "node_modules/cwebp-bin/.+\\.(j|t)s$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!cwebp-bin/.*)"
    ],
    "coverageReporters": [
      "lcov",
      "json-summary"
    ]
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint --ext .js --fix ./src",
      "git add"
    ]
  },
  "maintainers": [
    "ones"
  ],
  "publishConfig": {
    "registry": "https://npm2.myones.net/",
    "access": "public"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.19.4",
    "@babel/plugin-proposal-optional-chaining": "^7.18.9",
    "@babel/plugin-transform-runtime": "^7.19.1",
    "@babel/preset-env": "^7.19.4",
    "@babel/runtime": "^7.0.0-beta.55",
    "@types/jest": "^29.2.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^29.2.1",
    "babel-plugin-transform-import-meta": "^2.2.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "chalk": "^4.1.2",
    "eslint": "^8.25.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "jest": "^29.4.2",
    "rimraf": "^4.1.2",
    "standard-version": "^9.5.0"
  },
  "dependencies": {
    "async": "^3.2.4",
    "chrome-launcher": "^0.15.1",
    "echarts": "^5.4.2",
    "fs-extra": "^11.1.0",
    "glob": "^8.1.0",
    "joi": "^17.7.0",
    "json2md": "^2.0.0",
    "koa": "^2.14.1",
    "koa-static": "^5.0.0",
    "lighthouse": "^9.6.8",
    "marked": "^4.2.12",
    "progress": "^2.0.3",
    "shelljs": "^0.8.5",
    "sitespeed.io": "^26.1.0",
    "winston": "^3.8.2",
    "yargs": "^17.6.2"
  }
}
